    "version": "0.2.0",
    "configurations": [
      
        {
            "type": "chrome",
            "request": "launch",
            "name": "localhost",
            "url": "http://localhost:4000",
            "webRoot": "${workspaceFolder}/dist/index.js"
        },
        {
          "type": "node",
          "request": "launch",
          "name": "New Launch",
          "skipFiles": ["<node_internals>/**"],
          "program": "${workspaceFolder}\\index.js"
        }
    ]
}